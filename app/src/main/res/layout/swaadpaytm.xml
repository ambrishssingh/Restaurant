<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/material_dynamic_primary80"
    tools:context=".swaadFinalPaymentActivity">

    <TextView
        android:id="@+id/textView19"
        android:layout_width="140dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="207dp"
        android:layout_marginEnd="255dp"
        android:layout_marginBottom="479dp"
        android:text="Your Amount :"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/spaymentf"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:layout_marginStart="185dp"
        android:layout_marginTop="207dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="479dp"
        android:text=""
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        android:autofillHints="" />

    <Button
        android:id="@+id/sfinalpay"
        android:layout_width="200dp"
        android:layout_height="54dp"
        android:layout_marginStart="94dp"
        android:layout_marginTop="316dp"
        android:layout_marginEnd="117dp"
        android:layout_marginBottom="361dp"
        android:background="@drawable/buttonstyle"
        android:text="Do Payment"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/scancelpay"
        android:layout_width="201dp"
        android:layout_height="56dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="415dp"
        android:layout_marginEnd="117dp"
        android:layout_marginBottom="260dp"
        android:background="@drawable/buttonstyle"
        android:text="Cancel Payment"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>